use uo;
use os;

program on_remove(who, backpack, item)
  if(GetObjProperty(who,"backpack")==1)
    PrintTextAbovePrivate(backpack, "If you want it, try bidding on it.", who);
    return 0;
  else
    PrintTextAbovePrivate(backpack, "You will have to steal that.", who);
    return 0;
  endif
endprogram