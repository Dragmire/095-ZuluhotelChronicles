use uo;
use os;

program on_remove(who, backpack, item)
  if(who.cmdlevel>=2 || GetObjProperty(backpack,"casecity")!=1)
    return 1;
  else
    PrintTextAbovePrivate(backpack, "You will have to steal that.", who);
    return 0;
  endif
endprogram