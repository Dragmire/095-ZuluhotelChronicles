use uo;


program unlockradius( who, text )
    var ranget := cint( text );
    
    foreach thingie in ListItemsNearLocation( who.x, who.y, who.z, ranget );
        thingie.movable := 1;
    endforeach
    
    SendSysMessage(who, "Everything in a radius of " + ranget + " is now movable." );
        
endprogram
