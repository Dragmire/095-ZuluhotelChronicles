Use uo;
Use os;
Use util;

Program TextCMD_Whisper( Who )


    Var Tar := TargetCoordinates( who );
    If( !Tar )
    Return 0;
    Endif
    
    SendSysMessage( Who, "Coord" + Tar );
 
Endprogram
