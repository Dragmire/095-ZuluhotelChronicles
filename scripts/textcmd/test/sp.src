Use uo;
Use os;
Use math;
Use cfgfile;

Program textcmd_mount( who )

    Var Bag := CreateItemInBackpack( who, 0xE76, 1 );
    
    Foreach item in ListReagents()
    CreateItemInContainer( Bag, item, 10000 );
    Endforeach


Endprogram

