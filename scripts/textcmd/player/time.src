use uo;

include "include/time";

program textcmd_where( who )

    Var Seconds := polcore().systime;

    SendSysMessage( Who , "Today's Date is " + GetDateString( Seconds ) );
    
    SendSysMessage( Who , "Server Time is " + GetTimeString( Seconds ) );

endprogram
